{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Ger",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.instructions.ger",
				"match": "\\b(ger|GER|Ger|ge r|gEr|geR)\\b"
			}]
		},
		"strings": {
			"name": "string.semicolon.ger",
			"begin": "; ",
			"end": "\n",
			"patterns": [
				{
					"name": "constant.string_keywords.ger",
					"match": "\\b(insert_newline|insert_space)\\b"
				}
			]
		},
		"comments": {
			"name": "comment.exclamation.ger",
			"begin": "!= ",
			"end": "\n"
		}
	},
	"scopeName": "source.ger"
}