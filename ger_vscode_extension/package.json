{
    "name": "ger-highlight",
    "displayName": "Ger Highlight",
    "description": "",
    "version": "1.6.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "publisher": "Helo",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ger",
                "aliases": ["Ger", "ger"],
                "extensions": [".ger"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ger",
                "scopeName": "source.ger",
                "path": "./syntaxes/ger.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ger",
                "path": "./snippets/ger.json"
            }
        ]
    }
}
